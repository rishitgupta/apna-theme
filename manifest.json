{
    "name": "Apna Theme",
    "description": "Build your own theme for Chrome!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "1.png",
            "32": "2.png",
            "64": "3.png",
            "128": "4.png"
        }
    },
    "icons": {
        "16": "1.png",
        "32": "2.png",
        "64": "3.png",
        "128": "4.png"
    }
}